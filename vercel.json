{
  "version": 2,
  "builds": [
    { "src": "index.py", "use": "@vercel/python" }, 
    { "src": "static front-end/package.json", "use": "@vercel/static-build" }
  ],
  "routes": [
    { "src": "/api/(.*)", "dest": "index.py" },  
    { "src": "/(.*)", "dest": "static front-end/build/index.html" }
  ]
}
